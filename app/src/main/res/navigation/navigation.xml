<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/searchFragment">

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.grantzapp.activity.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_adminloginActivity"
            app:destination="@id/adminloginActivity" />
    </fragment>
    <activity
        android:id="@+id/adminloginActivity"
        android:name="com.example.grantzapp.activity.AdminloginActivity"
        android:label="activity_adminlogin"
        tools:layout="@layout/activity_adminlogin" />
</navigation>